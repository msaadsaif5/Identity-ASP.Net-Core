@model IEnumerable<ConfidentialCase>

<div class="bg-primary m-1 p-1 text-white">
    <h4>Your Cases (@User?.Identity?.Name)</h4>
</div>
<table class="table table-sm table-bordered">
    <tr>
        <th>Title</th>
        <th>Created By</th>
    </tr> 
    @foreach (var c in Model) {
    <tr>
        <td>@c.Title</td>
        <td>@c.CreatedBy</td>
    </tr>
}
</table>
    <a asp-action="Logout" asp-controller="Account" class="btn btn-danger">Logout</a>